Program.Sub.cmd_Print_Click.Start
F.Intrinsic.Control.SetErrorHandler("cmd_Print_Click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.iReportID.Declare(Long)
V.Local.sRows.Declare(String)
V.Local.iC.Declare(Long)
V.Local.iOrd.Declare(Long)
V.Local.sQuote.Declare(String)
V.Local.sParams.Declare(String)
V.Local.iRet.Declare(Long)
v.Local.sFile.Declare(String)

'Disappear the screen
Gui.SS_Form1..Visible(False)

F.Global.BI.GetIDFromName("GCG_3623_Quote.rpt",V.Local.iReportID)

v.Local.sRows.Set(v.Screen.SS_Form1!lvw_Quote.ListViewContents)
F.Intrinsic.Control.If(V.Local.sRows,<>,"***NORETURN***")
	F.Intrinsic.String.Split(V.Local.sRows,"$@$",V.Local.sRows)
	F.Intrinsic.Control.For(V.Local.iC,0,V.Local.sRows.UBound,1)
		f.Intrinsic.Control.If(v.Dictionary.Quote![V.Local.sRows(V.Local.iC)],<>,"***NORETURN***")
			F.Global.BI.GetRunID(V.Global.lBIRunID)
			F.Global.BI.StartLogging(V.global.lBIRunID,V.Local.iReportID,0,"",V.Global.lLogID)
			V.Local.sQuote.Set(V.Local.sRows(V.Local.iC))
			V.Local.sParams.Set("Quote")
			
			F.Global.BI.StopLogging(V.Global.lBIRunID)
		f.Intrinsic.Control.Else
			v.Local.iReportID.Set(1)
			F.Global.BI.GetRunID(V.Global.lBIRunID)
			F.Global.BI.StartLogging(V.global.lBIRunID,V.Local.iReportID,0,"",V.Global.lLogID)
'			V.Local.sQuote.Set(V.Local.sRows(V.Local.iC))
			f.Global.General.CallWrapperSyncBio(920000,v.Local.sRows(v.Local.iC))
			f.Intrinsic.String.Build("{0}*!*{1}*!*Y*!**!**!**!**!**!*",v.Caller.Terminal,"000001",v.Local.sQuote)
			V.Local.sParams.Set("Terminal*!*reportID*!*Rev*!*QtyDesc*!*PrText*!*PrtTot*!*PrtAll*!*Comments*!*logo")
			
			F.Global.BI.StopLogging(V.Global.lBIRunID)
		f.Intrinsic.Control.EndIf
		
		f.Intrinsic.Control.If(v.Global.bExport,=,False)
			F.Global.BI.RunReportPreProcessor(V.global.lBIrunID,V.global.lLogid,V.Local.sParams,v.Local.sRows(v.Local.iC),"",-1,True,"",-1,"",-1,"","",V.Local.iRet)
		f.Intrinsic.Control.Else
			f.Intrinsic.String.Build("{0}\3623_Quotes",v.Caller.FilesDir,v.Local.sFile)
			f.Intrinsic.File.CreateDir(v.Local.sFile)
			f.Intrinsic.String.Build("{0}\{1}.PDF",v.Local.sFile,v.Local.sRows(v.Local.iC),v.Local.sFile)
			F.Global.BI.RunReportPreProcessor(V.global.lBIrunID,V.global.lLogid,V.Local.sParams,V.Local.sQuote,"",4,True,"",1,"",0,v.Local.sFile,"",V.Local.iRet)
		f.Intrinsic.Control.EndIf
		
	F.Intrinsic.Control.Next(V.Local.iC)
F.Intrinsic.Control.EndIf



F.Intrinsic.Control.CallSub(ss_form1_unload)

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmd_Print_Click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_3623_Quote.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(ss_form1_unload)
Function.Intrinsic.Control.EndIf


