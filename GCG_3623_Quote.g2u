Program.Sub.Main.Start
F.Intrinsic.Control.SetErrorHandler("Main_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSql.Declare(String)
v.Local.sRet.Declare(String)

F.ODBC.Connection!con.OpenConnection(V.Ambient.PDSN,V.Ambient.PUser,V.Ambient.PPass)

V.Local.sSql.Set("Select Distinct Quote_No, Quote_No From GCG_3623_Quote")
F.Data.Dictionary.CreateFromSQL("Quote","con",V.Local.sSql,16)
f.Data.Dictionary.SetDefaultReturn("Quote","***NORETURN***")

F.Intrinsic.Control.If(v.Caller.Hook,<>,10940)
	f.Intrinsic.Control.If(v.Caller.Switches,=,"")
		Gui.SS_Form1.txt_Quote.AddAutoCompleteItem("Quote","Dictionary")
		Gui.SS_Form1..Show
	f.Intrinsic.Control.Else
		'Split the Switch to get the Quote number and Export Boolean
		f.Intrinsic.String.Split(v.Caller.Switches,"@!@",v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet.UBound,>,0)
			v.Global.bExport.Set(True)
		f.Intrinsic.Control.EndIf
'		Gui.SS_Form1.txt_Quote.Text(V.Caller.Switches.Trim)
		f.Intrinsic.String.LPad(v.Local.sRet.Trim,"0",7,v.Local.sRet)
		Gui.SS_Form1.txt_Quote.Text(v.Local.sRet.Trim)
		f.Intrinsic.Control.CallSub(cmd_lvw_quoteadd_click)
		f.Intrinsic.Control.CallSub(cmd_print_click)
	f.Intrinsic.Control.EndIf
F.Intrinsic.Control.Else
	Gui.SS_Form1.lvw_Quote.AddListItem(v.Passed.000003.Format0000000,v.Passed.000003.Format0000000)
	Gui.SS_Form1.txt_Quote.Text("")
	f.Intrinsic.Control.If(v.Dictionary.Quote![v.Passed.000003.Format0000000],<>,"***NORETURN***")
		v.Passed.777777.Set(1)
		F.Intrinsic.Control.CallSub(cmd_print_click)
	f.Intrinsic.Control.EndIf
	f.Intrinsic.Control.CallSub(ss_form1_unload)	
	
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("Main_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_3623_Quote.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(ss_form1_unload)
Function.Intrinsic.Control.EndIf


Program.Sub.Main.End

